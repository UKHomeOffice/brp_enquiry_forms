    <table role="presentation" class="x_content" align="center" cellpadding="0" cellspacing="0" border="0" width="100%" style="border-collapse:collapse; max-width:580px; width:100%!important">
      <tbody>
        <tr>
          <td width="10" height="10" valign="middle"></td>
          <td>
            <table role="presentation" width="100%" cellpadding="0" cellspacing="0" border="0" style="border-collapse:collapse">
              <tbody>
                <tr>
                  <td bgcolor="#1D70B8" width="100%" height="10"></td>
                </tr>
